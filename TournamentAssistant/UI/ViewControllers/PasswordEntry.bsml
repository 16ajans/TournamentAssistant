<vertical xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance' xsi:schemaLocation='https://monkeymanboy.github.io/BSML-Docs/ https://raw.githubusercontent.com/monkeymanboy/BSML-Docs/gh-pages/BSMLSchema.xsd'>
  <horizontal bg='panel-top' pad-left='10' pad-right='10' horizontal-fit='PreferredSize'>
    <text text='This server is password protected' align='Center' font-size='10'></text>
  </horizontal>
    <vertical background='round-rect-panel' id='Background'>
      <string-setting text='Password:' value='password' apply-on-change='true' align='Center'></string-setting>
    </vertical>
  <button on-click='connect' text='Connect' hover-hint='Connect to password-protected server'></button>
</vertical>